  <% @contacts.each do |contact| %>
        <h2>First Name: <a href= "/all_contacts/<%= contact.id %>" ><%= contact.first_name %></a></h2>
        <h2>Middle Name: <%= contact.middle_name  %></h2>
        <h2>Last Name: <%= contact.last_name %></h2>
        <h2>Email: <%= contact.email %></h2>
        <h2>Phone Number: <%= contact.phone_number %></h2>
        <h2>Bio: <%= contact.bio  %></h2>
        <br>
  <% end %>
